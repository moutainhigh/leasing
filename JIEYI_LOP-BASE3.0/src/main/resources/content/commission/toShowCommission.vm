<!DOCTYPE HTML>
<html>
<head>
	#parse("content/importBase.vm")
	<script type="text/javascript" src="$!_basePath/commission/js/commission.js"></script>
</head>
<body>
#foreach($app in $loanList)

	<table class="table_01" cellpadding="0" cellspacing="0">
		<tr>
			<td>项目类型</td>
			<td>商务政策别名 </td>
			<td>政策类型 </td>
			<td>支付表号 </td>
			<td>项目编号 </td>
			<td>客户名称 </td>
			<td>年利率 </td>
			<td>经销商名 </td>
			<td>车系车型 </td>
			<td>设备总价值 </td>
			<td>融资额 </td>
			<td>期次  </td>
			<td>放款通过日期  </td>
			<td>超融  </td>
			<td>设备数量  </td>
			<td>预计返佣日期  </td>
			<td>收款单位  </td>
			<td>开户行  </td>
			<td>开户行账号  </td>
			<td>结算状态  </td>
			<td>返佣金额 </td>
		</tr>
		#foreach($item in $app.rebateList)
		<tr>
			<td>$!item.PLATFORM_TYPE</td>
			<td>$!item.SCHEME_NAME </td>
			<td>$!item.ALIASES </td>
			<td>$!item.PAYLIST_CODE </td>
			<td>$!item.PRO_CODE </td>
			<td>$!item.NAME </td>
			<td>$!item.YEAR_INTEREST </td>
			<td>$!item.SUP_NAME </td>
			<td>$!item.CXCX </td>
			<td>$!item.LEASE_TOPRIC </td>
			<td>$!item.FINANCE_TOPRIC </td>
			<td>$!item.LEASE_TERM  </td>
			<td>$!item.BEGGIN_DATE  </td>
			<td>$!item.CR  </td>
			<td>$!item.FEQNUM  </td>
			<td>$!item.PREDICT_DATE  </td>
			<td>$!item.PAYEE  </td>
			<td>$!item.BANK  </td>
			<td>$!item.BANK_ACCOUNT </td>
			<td>$!item.STATUS </td>
			<td>$!item.REBATE_AMOUNT  </td>
		</tr>
		#end
	</table>
	<br/>
	<table class="table_01 mytable" cellpadding="0" cellspacing="0" width="200px">
		<tr>
			<td align="center"  bgcolor="#fbfbfb" colspan="3">付款情况说明</td>
		</tr>
		<tr>
			<td width="20%">合计金额（小写）</td>
			<td width="20%">合计金额（大写）</td>
			<td width="60%">备注</td>
		</tr>
		<tr>
			<td><input type="text" name="AMOUNT_TOTAL" value="$!app.AMOUNT_TOTAL" disabled style="width:150px"></td>
			<td><input type="text" name="UPPER" value="$!app.UPPER" disabled style="width:150px"></td>
			<td>
				<input type="text" name="REMARK" value="$!app.REMARK" style="width:200px" disabled/>
			</td>
		</tr>
	</table>
	<br/>
#end

</body>
</html>
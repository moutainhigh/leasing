<script type="text/javascript">
	$(function(){
		$(".linkbutton").linkbutton();
		$(".autoPanel").panel();
		$(".datebox").datebox();
		$(".datagrid").datagrid();
		$(".dialog").dialog();
	});
	
	$("input").each(function() {
			$(this).attr("readonly","true");
    	});
		
		$(".FOMATINPUT input").each(function() {
			var st=$(this).attr("style");
			$(this).attr("style",st+";text-align:right");
    		$(this).formatCurrency();
    	});
		
		$(".FOMATINPUTTD input").each(function() {
			var st=$(this).attr("style");
			$(this).attr("style",st+";text-align:right");
    		$(this).formatCurrency();
    	});
</script>

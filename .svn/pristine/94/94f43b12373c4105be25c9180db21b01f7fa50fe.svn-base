						#foreach($item in $!type.INDUSTRY_FICATION_List)
    					<option value="$!item.CODE" #if($!item.CODE ==  $!custInfo.INDUSTRY_FICATION)selected#end >$!item.FLAG</option>
						#end
				</select><font color="red">*</font></td>
        </tr>
		

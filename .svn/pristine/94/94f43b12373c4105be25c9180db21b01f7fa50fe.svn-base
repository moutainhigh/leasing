				#end
		    </td>
         </tr>
    	 <tr>
            <td class="text_right" colspan="" width="20%">工作单位：</td>
			<td colspan="5" width="80%"><input type="text" size="85" name="WORK_UNIT" id="WORK_UNIT" value="$!custInfo.WORK_UNIT" /></td>

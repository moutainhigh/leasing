    		<td class="text_right" colspan="" width="20%">传真：</td>
            <td colspan="" width="20%"><input type="text" name="FAX" id="FAX" value="$!custInfo.FAX"  class="validate[custom[fax]]" onchange="isFax('FAX');"/></td>

	<DIV style="height:20PX"></DIV>
	<div title="企业关联" id="business" class="ui-panel" data-options="href:'$_basePath/customers/CustMainRelation!toMgEnterpriseRel.action?CLIENT_ID=$!param.CLIENT_ID&TYPE=$!param.TYPE&tab=enterprise&tab_1=view',cache:false,tools:[{iconCls:'icon-reload',handler:function(){$('#business').panel('refresh', '$_basePath/customers/CustMainRelation!toMgEnterpriseRel.action?CLIENT_ID=$!param.CLIENT_ID&TYPE=$!param.TYPE&tab=enterprise&tab_1=update');}}]" >
    </div>

    <div title="企业团队" id="EnterpriseTeam"  class="ui-panel"   data-options="href:'$_basePath/customers/CustMainRelation!toMgEnterpriseTeam.action?CLIENT_ID=$!param.CLIENT_ID&tab_1=view',cache:false,tools:[{iconCls:'icon-reload',handler:function(){$('#EnterpriseTeam').panel('refresh', '$_basePath/customers/CustMainRelation!toMgEnterpriseTeam.action?CLIENT_ID=$!param.CLIENT_ID&tab_1=update');}}]" >

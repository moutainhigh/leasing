package com.pqsoft.creditData.model;

import com.google.common.base.Strings;

import java.math.BigDecimal;
import java.util.regex.Pattern;

/**
 * 
 * 职业信息
 * 
 * @className Occupation
 * @author 刘丽
 * @version V1.0 2016年4月21日 下午3:40:11
 */
public class Occupation extends BaseModel {

    private static final long serialVersionUID = 1L;
    private Identity identity;
    private String occupation;
    private String workingPlace;
    private String workingIndustry;
    private String workingProvince;
    private String workingCity;
    private String workingCounty;
    private String workingAddress;
    private String workingAddressZipCode;
    private String startYearInWorkingPlace;
    private String post;
    private String title;
    private BigDecimal annualIncome;


    /**
     * 得到职业
     */
    public String getOccupation() {
        return occupation;
    }

    /**
     * 设置职业
     */
    public void setOccupation(String occupation) {
        this.occupation = occupation;
    }

    public Identity getIdentity() {
        return identity;
    }

    public void setIdentity(Identity identity) {
        this.identity = identity;
    }

    /**
     * 得到单位名称
     */
    public String getWorkingPlace() {
        return workingPlace;
    }

    /**
     * 设置单位名称
     */
    public void setWorkingPlace(String workingPlace) {
        this.workingPlace = workingPlace;
    }

    /**
     * 得到单位所属行业
     */
    public String getWorkingIndustry() {
        return workingIndustry;
    }

    /**
     * 设置单位所属行业
     */
    public void setWorkingIndustry(String workingIndustry) {
        this.workingIndustry = workingIndustry;
    }

    /**
     * 得到单位地址
     */
    public String getWorkingAddress() {
        return workingAddress;
    }

    /**
     * 设置单位地址
     */
    public void setWorkingAddress(String workingAddress) {
        this.workingAddress = workingAddress;
    }

    /**
     * 得到单位地址邮政编码
     */
    public String getWorkingAddressZipCode() {
        return workingAddressZipCode;
    }

    /**
     * 设置单位地址邮政编码
     */
    public void setWorkingAddressZipCode(String workingAddressZipCode) {
        this.workingAddressZipCode = workingAddressZipCode;
    }

    /**
     * 得到本单位工作起始年份
     */
    public String getStartYearInWorkingPlace() {
        return startYearInWorkingPlace;
    }

    /**
     * 设置本单位工作起始年份
     */
    public void setStartYearInWorkingPlace(String startYearInWorkingPlace) {
        this.startYearInWorkingPlace = startYearInWorkingPlace;
    }

    /**
     * 得到职务
     */
    public String getPost() {
        return post;
    }

    /**
     * 设置职务
     */
    public void setPost(String post) {
        this.post = post;
    }

    /**
     * 得到职称
     */
    public String getTitle() {
        return title;
    }

    /**
     * 设置职称
     */
    public void setTitle(String title) {
        this.title = title;
    }

    /**
     * 得到年收入(系统数据不规则，不做查询)
     */
    public BigDecimal getAnnualIncome() {
       return annualIncome;
    }

    /**
     * 设置年收入
     */
    public void setAnnualIncome(BigDecimal annualIncome) {
        this.annualIncome = annualIncome;
    }

    public String getWorkingProvince() {
        return workingProvince;
    }

    public void setWorkingProvince(String workingProvince) {
        this.workingProvince = workingProvince;
    }

    public String getWorkingCity() {
        return workingCity;
    }

    public void setWorkingCity(String workingCity) {
        this.workingCity = workingCity;
    }

    public String getWorkingCounty() {
        return workingCounty;
    }

    public void setWorkingCounty(String workingCounty) {
        this.workingCounty = workingCounty;
    }

    @Override
    public String toString() {
        return "Occupation{" +
                "identity=" + identity +
                ", occupation='" + occupation + '\'' +
                ", workingPlace='" + workingPlace + '\'' +
                ", workingIndustry='" + workingIndustry + '\'' +
                ", workingProvince='" + workingProvince + '\'' +
                ", workingCity='" + workingCity + '\'' +
                ", workingCounty='" + workingCounty + '\'' +
                ", workingAddress='" + workingAddress + '\'' +
                ", workingAddressZipCode='" + workingAddressZipCode + '\'' +
                ", startYearInWorkingPlace='" + startYearInWorkingPlace + '\'' +
                ", post='" + post + '\'' +
                ", title='" + title + '\'' +
                ", annualIncome=" + annualIncome +
                "} " + super.toString();
    }
}

	<form id="holderUpdate" method="post">
		<input type="hidden" name="HOLDER_ID" id="HOLDER_ID" value="$!holder.HOLDER_ID">
         <table cellpadding="0" cellspacing="0" class="table_01">
            <tr>
                <td >股东名称：</td>
                <td ><input type="text" name="HOLDER_NAME" id="HOLDER_NAME" value="$!holder.HOLDER_NAME"/></td>
                <td>投资金额：</td>
				<td ><input type="text" name="HOLDER_CAPITAL" id="HOLDER_CAPITAL" value="$!holder.HOLDER_CAPITAL" class="validate[required,custom[onlyNumber]]"/>万元</td>
			</tr>
			<tr>
                <td>投资方式：</td>
                <td><input type="text" name="HOLDER_WAY" id="HOLDER_WAY" value="$!holder.HOLDER_WAY"/></td>
                 <td>所占比例：</td>
                <td colspan=""><input type="text" style="" name="HOLDER_RATE" id="HOLDER_RATE" value="$!holder.HOLDER_RATE" class="validate[required,custom[onlyNumber]]"/>%</td>
			</tr>
			<tr >
				<td>备注：</td>
				<td colspan="3">
					<textarea name="HOLDER_MOME" style="width:460px;">$!holder.HOLDER_MOME</textarea>
                </td>
            </tr>
		 </table>
    </form>
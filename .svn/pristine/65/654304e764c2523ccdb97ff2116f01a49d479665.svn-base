body {
	margin: 0;
}

ul li {
	list-style: none outside none;
}

.lazyTabContentsLeft {
	background-color: #F0F8FF;
	display: none;
	float: left;
	position: relative;
}

.lazyTabHeaderLeft {
	background-color: #FFFFFF;
	border-right: 0 solid #B9D3EE;
	display: block;
	float: left;
	font-size: 93%;
	height: 100%;
	left: 0;
	line-height: normal;
	overflow: hidden;
	padding-right: 3px;
	position: relative;
	top: 0;
	width: 35px;
}

.lazyTabHeaderLeft ul {
	background-color: #C6E2FF;
	border-right: 1px solid #B9D3EE;
	font: 12px sans-serif;
	height: 100%;
	list-style: none outside none;
	margin: 0;
	padding-left: 10px;
	position: relative;
}

.lazyTabHeaderLeft li {
	background: url("../img/tabLeft.png") no-repeat scroll 0 0 transparent;
	margin: 2px 0 0;
	overflow: hidden;
	padding: 10px 0 0;
	position: relative;
	width: 100%;
}

.lazyTabHeaderLeft .lazyTabLink {
	background: url("../img/tabLeft.png") no-repeat scroll -120px bottom
		transparent;
	color: #43798B;
	display: block;
	height: 100%;
	outline: medium none;
	padding: 0 0 10px;
	text-align: center;
	text-decoration: none;
}

.lazyTabHeaderLeft .lazyTabLink span {
	background: url("../img/tabLeft.png") repeat-y scroll -60px bottom
		transparent;
	display: block;
	padding: 6px 6px 6px 7px;
}

.lazyTabHeaderLeft li.active {
	background-position: -180px top;
	display: block;
	font-weight: bold;
	margin-right: -1px;
	padding-right: 1px;
	position: relative;
}

.lazyTabHeaderLeft li.active .lazyTabLink {
	background-position: -300px bottom;
	margin-right: -1px;
}

.lazyTabHeaderLeft li.active .lazyTabLink span {
	background-position: -240px top;
}

.lazyTabHeaderLeft li:hover,.lazyTabHeaderLeft li.hover {
	background-position: -180px top;
	color: #333333;
}

.lazyTabHeaderLeft li:hover .lazyTabLink,.lazyTabHeaderLeft li.hover .lazyTabLink
	{
	background-position: -300px bottom;
}

.lazyTabHeaderLeft li:hover .lazyTabLink span,.lazyTabHeaderLeft li.hover .lazyTabLink span
	{
	background-position: -240px top;
}

.lazyTabHeaderLeft .lazyTabClose {
	background: url("../img/closebg.png") no-repeat scroll left top
		transparent;
	bottom: 5px;
	height: 16px;
	left: 5px;
	position: absolute;
	width: 16px;
}

.lazyTabHeaderLeft li:hover .lazyTabClose:hover,.lazyTabHeaderLeft li.hover .lazyTabClose.hover
	{
	background: url("../img/closebg.png") repeat scroll -15px 0 transparent;
}

.lazyTabHeaderLeft .lazyTabScroll {
	padding-bottom: 17px;
	padding-top: 17px;
}

.lazyTabHeaderLeft .scrollTop {
	background: none repeat scroll 0 0 #C6E2FF;
	border-bottom: 0 solid #B9D3EE;
	border-right: 1px solid #B9D3EE;
	cursor: pointer;
	height: 17px;
	left: 0;
	opacity: 0.9;
	position: absolute;
	top: 0;
	width: 100%;
	z-index: 10;
}

.lazyTabHeaderLeft .scrollBottom {
	background: none repeat scroll 0 0 #C6E2FF;
	border-right: 0 solid #B9D3EE;
	border-top: 0 solid #B9D3EE;
	bottom: 0;
	cursor: pointer;
	height: 16px;
	left: 0;
	opacity: 0.9;
	position: absolute;
	width: 100%;
	z-index: 10;
}

.lazyTabHeaderTop .disabled {
	opacity: 0.5;
}
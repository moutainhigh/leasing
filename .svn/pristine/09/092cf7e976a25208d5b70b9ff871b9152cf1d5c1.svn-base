package com.pqsoft.util;

/**
 * 发票条目
 * @author ITDevelop
 *
 */
public class InvoiceItem {
	private String Item_id;//明细项id
	private String Saleitem_Invoice_Id;//
	private String Item_Name;//商品名称
	private Double Item_Unit_Price;//单价
	private Double Item_Num;//数量
	private Double Item_Fact_Amt;//含税金额  实际金额
	private Double Item_Sub_Amt;//金额
	private Double Item_Sub_Tax_Amt;//税额
	private String Item_Model;//规格型号
	private String Item_Tax_Rate;//税率
	private String Item_unit;//计量单位
	
	public String getItem_id() {
		return Item_id;
	}
	public void setItem_id(String itemId) {
		Item_id = itemId;
	}
	public String getSaleitem_Invoice_Id() {
		return Saleitem_Invoice_Id;
	}
	public void setSaleitem_Invoice_Id(String saleitemInvoiceId) {
		Saleitem_Invoice_Id = saleitemInvoiceId;
	}
	public String getItem_Name() {
		return Item_Name;
	}
	public void setItem_Name(String itemName) {
		Item_Name = itemName;
	}
	public Double getItem_Unit_Price() {
		return Item_Unit_Price;
	}
	public void setItem_Unit_Price(Double itemUnitPrice) {
		Item_Unit_Price = itemUnitPrice;
	}
	public Double getItem_Num() {
		return Item_Num;
	}
	public void setItem_Num(Double itemNum) {
		Item_Num = itemNum;
	}
	public Double getItem_Fact_Amt() {
		return Item_Fact_Amt;
	}
	public void setItem_Fact_Amt(Double itemFactAmt) {
		Item_Fact_Amt = itemFactAmt;
	}
	public Double getItem_Sub_Amt() {
		return Item_Sub_Amt;
	}
	public void setItem_Sub_Amt(Double itemSubAmt) {
		Item_Sub_Amt = itemSubAmt;
	}
	public Double getItem_Sub_Tax_Amt() {
		return Item_Sub_Tax_Amt;
	}
	public void setItem_Sub_Tax_Amt(Double itemSubTaxAmt) {
		Item_Sub_Tax_Amt = itemSubTaxAmt;
	}
	public String getItem_Model() {
		return Item_Model;
	}
	public void setItem_Model(String itemModel) {
		Item_Model = itemModel;
	}
	public String getItem_Tax_Rate() {
		return Item_Tax_Rate;
	}
	public void setItem_Tax_Rate(String itemTaxRate) {
		Item_Tax_Rate = itemTaxRate;
	}
	public String getItem_unit() {
		return Item_unit;
	}
	public void setItem_unit(String itemUnit) {
		Item_unit = itemUnit;
	}
	
}

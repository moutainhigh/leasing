	<!-- 删除承租人资产负债表数据 -->
	<delete id="deleteDebt" parameterType="map">
		delete from
		FIL_PROJECT_FINANCE_DEBT f where f.PROJECT_ID=#{PROJECT_ID} and
		f.TYPE=0
	</delete>


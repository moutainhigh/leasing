	<select id="getFinnaceDebtRisk" parameterType="map" resultType="map">
		select
		FLOW_ASSETS_TOTAL,FLOW_DABT_TOTAL,STOCK,ACCOUNTS_SECTION,NET_TOTAL,FIXED_NET,
		DABT_TOTAL,OWER_TOTAL,INVISIBLE_NET from FIL_PROJECT_FINANCE_DEBT
		where PROJECT_ID=#{PROJECT_ID}
   </select>

	<select id="getFinnaceProfitDisRisk" parameterType="map"
		resultType="map">
		select
		MAIN_COST,NET_SALES,MAIN_INCOME,PROFIT_TOTAL,INTEREST_EXPENSES,PROFIT_NET,
		OPER_PROFIT,MAIN_OPER_COST,MAIN_MAN_FEE,MAIN_FINNA_FEE,NOT_ASSIGNED_PROFIT,
		STATU_SURPLUS_RESERVE,STATU_SURPLUS_FUND,EXTRACTION_SURPLUS_RESERVE
		from FIL_PROJECT_FINANCE_PROFITDIS where PROJECT_ID=#{PROJECT_ID} and
		STATE=0
   </select>

	<select id="getFinnaceProfitDistriButtion" parameterType="map"
		resultType="map">
		select
		ID,PROJECT_ID,CREADE_CODE,CREADE_DATE,STATE,UNIT_NAME,CHECK_PEOPLE,to_char(CHECK_DATE,'yyyy-mm-dd')
		CHECK_DATE,REVIEW_PEOPLE,to_char(REVIEW_DATE,'yyyy-mm-dd')

	<select id="getFinnaceDebt" parameterType="map" resultType="map">
		select
		NOT_PROFIT,CURRENCY_FUNDS,NOTE_TOTAL,ASSENT_DAMAGED,WAIT_FEE,HOLD_SECTION,DEFERRED_TEX_ITEM,DEFERRED_TAX,STOCK
		from FIL_PROJECT_FINANCE_DEBT where PROJECT_ID=#{PROJECT_ID}
   </select>

	<select id="getFinnaceCash" parameterType="map" resultType="map">
		SELECT ID,UNIT_NAME,CHECK_PEOPLE,to_char(CHECK_DATE,'yyyy-mm-dd')

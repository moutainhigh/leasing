body{ margin:0; padding:0; width:100%; height:auto; font-family:"微软雅黑"; font-size:12px;}
img{ border:0; max-width:100%;}
.fl{ float:left;}
.fr{ float:right;}


.title{ width:100%;height:80px; background:#48474d; line-height:25px; color:#FFFFFF;position:fixed; top:0; z-index:1;}
.title1{ width:33.33%; height:80px; text-align:center;}
.title1 img{ margin-top:15px;}
.content{ width:100%; height:auto; margin-top:80px;}
.table01{ width:33.1%; height:106px; border-bottom:1px solid #e2e2e2; border-right:1px solid #e2e2e2; text-align:center;color:#29282f; line-height:28px; font-size:14px;}
.table01 img{ max-width:50%; margin-top:30px;}
.radiu-text{  position: absolute; top: 10%;right: 10%; width:20px; height:20px; border-radius:20px; background:#bc0404; color:#fff;
		line-height:20px;} 

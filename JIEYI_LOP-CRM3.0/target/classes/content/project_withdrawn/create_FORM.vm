<title>作废申请</title>
 #parse("content/importBase.vm")
 <script type="text/javascript" src="$_basePath/project_withdrawn/js/withdrawn.js"></script>
 
     <table  cellpadding="0" cellspacing="0" class="table_01">
    	<tr>
            <td class="text_right">项目编号</td>
    		<td>$!param.PROJECT_CODE &nbsp;</td>
            <td class="text_right">客户名称</td>
            <td>$!param.CUST_NAME &nbsp;</td>
		</tr>
		<tr>
			<td class="text_right">厂商</td>
            <td>$!param.COMPANY_NAMES &nbsp;</td>
			<td class="text_right">供应商</td>
            <td>$!param.SUPPLIER_NAMES &nbsp;</td>
        </tr>
	</table>
	
	<DIV style="height:20PX"></DIV>
	
    <form id="borrowform">
		<input name="SUPPLIER_ID" id="SUPPLIER_ID" value="$!param.SUP_ID" type="hidden">
		<input name="PROJECT_ID" id="PROJECT_ID" value="$!param.PROJECT_ID" type="hidden"/> 
		<input name="PROJECT_CODE" id="PROJECT_CODE" value="$!param.PROJECT_CODE" type="hidden"/>
		<input name="STATUS" id="STATUS" value="$!param.STATUS" type="hidden"/>
		<input name="CLIENT_ID" id="CLIENT_ID" value="$!param.CLIENT_ID" type="hidden"/>
		<input name="PLAN_STATUS" id="PLAN_STATUS" value="$!param.PLAN_STATUS" type="hidden"/>
		<table  cellpadding="0" cellspacing="0" class="table_01">
		#if($!param.STATUS=='2')
			
		#else
			#set($YSMoney=0)
			<tr>
                <td   colspan="6">应收金额</td>
            </tr>
			#if($!param.STATUS=='3')
				<tr>
					<td class="text_right">已放金额：#set($YSMoney=$YSMoney+$!BaseInfo.PAYMENT_MONEY)</td>
					<td><input name="PAYMENT_MONEY" id="PAYMENT_MONEY" type="$!BaseInfo.PAYMENT_MONEY" readonly/> &nbsp;</td>
					<td class="text_right">&nbsp;</td>
					<td> &nbsp;</td>
				</tr>
			#else
				<tr>
					<td class="text_right">逾期期次：#set($YSMoney=$YSMoney+$!BaseInfo.PAYMENT_MONEY)</td>
					<td><input name="DUN_NUM" id="DUN_NUM" value="$!BaseInfo.DUN_NUM" type="text" readonly/> &nbsp;</td>
					<td class="text_right">逾期租金：#set($YSMoney=$YSMoney+$!BaseInfo.DUN_BEGGING_MONEY)</td>
					<td><input name="DUN_BEGGING_MONEY" id="DUN_BEGGING_MONEY" value="$!BaseInfo.DUN_BEGGING_MONEY" readonly/> &nbsp;</td>
				</tr>
				<tr>
					<td class="text_right">罚息金额：#set($YSMoney=$YSMoney+$!BaseInfo.DUN_MONEY)</td>
					<td><input name="DUN_MONEY" id="DUN_MONEY" value="$!BaseInfo.DUN_MONEY" readonly/> &nbsp;</td>
					<td class="text_right">剩余本金：#set($YSMoney=$YSMoney+$!BaseInfo.PRINCIPAL_MONEY)</td>
					<td><input name="PRINCIPAL_MONEY" id="PRINCIPAL_MONEY" value="$!BaseInfo.PRINCIPAL_MONEY" readonly/> &nbsp;</td>
				</tr>
			#end
			<tr>
				<td colspan="3">&nbsp;</td>
				<td class="text_right">应收合计： <input name="RECEIVABLE_MONEY" id="RECEIVABLE_MONEY" readonly value="$!YSMoney"/> &nbsp;</td>
			</tr>
		#end
			
			<tr>
                <td  colspan="4">应付金额</td>
            </tr>
			<tr>
				<td  colspan="4">
                    #foreach($item in $fee_List)
						<input type="checkBox" name="$!item.KEY_NAME_EN"  class="checktype" value="$!item.VALUE_STR" ZN_NAME="$!item.KEY_NAME_ZN " onchange="checkCount()"/>$!item.KEY_NAME_ZN (金额：$!item.VALUE_STR) &nbsp;&nbsp;&nbsp;&nbsp;
    				#end
				</td>
            </tr>
			<tr>
				<td colspan="3">&nbsp;</td>
				<td class="text_right">应付合计： <input name="PAYABLE_MONEY" id="PAYABLE_MONEY" readonly value="0"/> &nbsp;</td>
			</tr>
			<tr>
                <td  colspan="4">收款方信息</td>
            </tr>
			<tr>
				<td class="text_right">收款单位：</td>
        		<td><input name="PAYEE_NAME" id="PAYEE_NAME" type="text"/></td>
                <td class="text_right">开户行:</td>
                <td><input name="RECE_BANK" id="RECE_BANK" type="text"/> &nbsp;</td>
            </tr>
			<tr>
				<td class="text_right">开户行地址：</td>
        		<td><input name="RECE_BANK_ADDREES" id="RECE_BANK_ADDREES" type="text"/></td>
                <td class="text_right">收款账号:</td>
                <td><input name="RECE_ACCOUNT" id="RECE_ACCOUNT" type="text"/> &nbsp;</td>
            </tr>
			<tr>
                <td  colspan="4">实付信息</td>
            </tr>
			<tr>
				<td class="text_right">计划付款金额：</td>
        		<td><input name="PLAN_MONEY" id="PLAN_MONEY" type="text"/></td>
                <td class="text_right">计划付款时间:</td>
                <td><input name="PLAN_DATE" id="PLAN_DATE" type="text" class="easyui-datebox"/> &nbsp;</td>
            </tr>
        </table>
    </form>
    </table>
</form>
	<div style="padding:20px;text-align:center">
        <a href="javascript:void('0');" class="easyui-linkbutton" data-options="iconCls:'icon-application'"id="addRecordButton" onclick="JBPM_START()">发起申请流程</a>
    </div>
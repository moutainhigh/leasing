<table  cellpadding="0" cellspacing="0" class="table_02">
	<thead >
    	<tr>
            <td class="text_left">票据对象</td>
            <td class="text_left">票据类型</td>
            <td class="text_left">设备名称</td>
            <td class="text_left">型号</td>
            <td class="text_left">发动机编号</td>
            <td class="text_left">开票日期</td>
            <td class="text_left">开票金额</td>
        </tr>
    </thead>
	<tbody >
    	#foreach($!item in $!PJ)
    	<tr>
            <td>$!item.INVOICE_NAME</td>
            <td>$!item.INVOICE_TYPE</td>
            <td>$!item.PRODUCT_NAME</td>
            <td>$!item.SPEC_NAME</td>
            <td>$!item.ENGINE_TYPE</td>
            <td>$!item.INVOICE_DATE</td>
            <td>$!item.INVOICE_AMT</td>
        </tr>
    	#end
    </tbody>
</table>